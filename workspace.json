{
  "version": 2,
  "projects": {
    "common": {
      "root": "libs/common",
      "sourceRoot": "libs/common/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/common/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/common"],
          "options": {
            "jestConfig": "libs/common/jest.config.js",
            "passWithNoTests": true
          }
        },
        "tsc": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn run tsc -- -b libs/common/tsconfig.lib.json --incremental"
          }
        }
      }
    },
    "reports": {
      "root": "apps/reports",
      "sourceRoot": "apps/reports/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/reports",
            "index": "apps/reports/src/index.html",
            "main": "apps/reports/src/index.tsx",
            "polyfills": "apps/reports/src/polyfills.ts",
            "tsConfig": "apps/reports/tsconfig.app.json",
            "assets": ["apps/reports/src/favicon.ico", "apps/reports/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "config/webpack/webpackCustomized.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/reports/src/environments/environment.ts",
                  "with": "apps/reports/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "reports:build",
            "hmr": true,
            "host": "dev-localhost.cqloud.com",
            "port": 8001,
            "ssl": true,
            "sslCert": "config/devServer/certificates/dev-localhost.cqloud.com.pem",
            "sslKey": "config/devServer/certificates/dev-localhost.cqloud.com-key.pem"
          },
          "configurations": {
            "production": {
              "buildTarget": "reports:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/reports/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/reports"],
          "options": {
            "jestConfig": "apps/reports/jest.config.js",
            "passWithNoTests": true
          }
        },
        "tsc": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "yarn run tsc -b apps/reports/tsconfig.app.json --incremental"
          }
        },
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/reports/e2e/cypress.json",
            "tsConfig": "apps/reports/e2e/tsconfig.e2e.json",
            "devServerTarget": "reports:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "reports:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "reports"
}
